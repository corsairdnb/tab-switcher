{
  "name": "Alt+Q Tab Switcher",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "Allows to quickly switch between 2 recent tabs using Alt+Q shortcut",
  "homepage_url": "https://github.com/corsairdnb/tab-switcher",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "switch-tab": {
      "suggested_key": {
        "default": "Alt+Q",
        "mac": "Command+Q"
      },
      "description": "Switch tab"
    }
  }
}
